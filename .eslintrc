{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "no-unused-vars": [1, { "argsIgnorePattern": "res|next|^err" }],
    "comma-dangle": 0,
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "import/no-dynamic-require": 0
  },
  "parserOptions": {"ecmaVersion": 6}
}